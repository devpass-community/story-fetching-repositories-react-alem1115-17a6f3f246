{
    "root": true,
    "extends": [
        "eslint:recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
        "no-unused-vars": [
            "warn",
            {
                "vars": "all",
                "args": "after-used",
                "ignoreRestSiblings": false
            }
        ]
    }
}